  },

  {
    "Key": "LW.Cunning.Name",
    "ProcessTemplates": false,
    "enGB": "Cunning (+1)"
  },
  {
    "Key": "LW.Cunning.Description",
    "enGB": "Whenever the weapon’s attack is a critical threat, the wielder gains a +4 bonus on the confirmation roll if she has 5 or more ranks in the Knowledge or Lore skill used to identify the target’s creature type, or a +6 bonus instead if she has 15 or more ranks."
  },

  {
    "Key": "LW.DazzlingRadiance.Name",
    "ProcessTemplates": false,
    "enGB": "Dazzling Radiance (+1)"
  },
  {
    "Key": "LW.DazzlingRadiance.Description",
    "enGB": "When a character wielding a dazzling radiance weapon uses the Dazzling Display feat, the weapon flashes in a burst of bright color. All enemies within 15 feet of the wielder who can see the display must attempt a Fortitude save. Those who fail are blinded for 2 rounds and dazzled for an additional 1d4 rounds; those who succeed are instead dazzled for 1d4 rounds.\nThe DC of the save is 17 + Mythic Rank."
  },

